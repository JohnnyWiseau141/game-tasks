<h1><%= title %></h1>

<form action="/games/<%= game._id %>?_method=PUT" method="POST">
  <input type="text" name="title" value="<%= game.title %>">
  <input type="checkbox" name="complete" <%= game.complete ? 'checked' : '' %>>
  <button type="submit">Update game</button>
</form>

